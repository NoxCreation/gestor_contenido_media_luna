{
  "kind": "collectionType",
  "collectionName": "shops",
  "info": {
    "singularName": "shop",
    "pluralName": "shops",
    "displayName": "Tiendas",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "descripcion": {
      "type": "text",
      "required": true
    },
    "usuario": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "pais": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::country.country"
    },
    "banner": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "clasificacion_tiendas": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::classification-store.classification-store",
      "inversedBy": "tiendas"
    },
    "whatsapp_contact": {
      "type": "string",
      "unique": true
    },
    "telegram_contact": {
      "type": "string",
      "unique": true
    },
    "instagram_contact": {
      "type": "string",
      "unique": true
    },
    "x_contact": {
      "type": "string",
      "unique": true
    },
    "phone_contact": {
      "type": "string",
      "unique": true
    },
    "site_contact": {
      "type": "string",
      "unique": true
    },
    "email_contact": {
      "type": "string",
      "unique": true
    },
    "productos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::stock.stock",
      "mappedBy": "tienda"
    },
    "estados": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::state.state"
    },
    "slug": {
      "type": "uid",
      "targetField": "nombre"
    },
    "precio_maximo": {
      "type": "decimal"
    },
    "precio_minimo": {
      "type": "decimal"
    },
    "mensajerias": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::mensajeria.mensajeria",
      "mappedBy": "tienda"
    },
    "monedas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::moneda.moneda"
    },
    "color_primario": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "cambio_moneda_tiendas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cambio-moneda-tienda.cambio-moneda-tienda",
      "mappedBy": "tienda"
    }
  }
}
