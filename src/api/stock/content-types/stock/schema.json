{
  "kind": "collectionType",
  "collectionName": "stocks",
  "info": {
    "singularName": "stock",
    "pluralName": "stocks",
    "displayName": "Stock",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "unique": false,
      "required": true
    },
    "foto1": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "precio": {
      "type": "decimal",
      "required": true
    },
    "clasificacion_productos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::clasification-product.clasification-product"
    },
    "comentarios": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comentarie.comentarie",
      "mappedBy": "stock"
    },
    "foto2": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "foto3": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "foto4": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "cantidad": {
      "type": "integer"
    },
    "descripcion": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "light"
      },
      "required": true,
      "customField": "plugin::ckeditor.CKEditor"
    },
    "tienda": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::shop.shop",
      "inversedBy": "productos"
    },
    "descuento_porciento": {
      "type": "integer"
    },
    "promedioValoracion": {
      "type": "decimal"
    }
  }
}
